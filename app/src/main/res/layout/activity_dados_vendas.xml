<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fbf0d8"
    android:backgroundTint="#fbf0d8"
    tools:context=".DadosVendas">

    <TextView
        android:id="@+id/txtVendas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="21dp"
        android:text="Insira o total vendido:"
        app:layout_constraintBottom_toTopOf="@+id/EdiValorLiquido"
        app:layout_constraintStart_toStartOf="@+id/EdiValorLiquido" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="16dp"
        android:text="Insira a quantidade liquida vendida"
        app:layout_constraintBottom_toTopOf="@+id/EdiQtdLiq"
        app:layout_constraintStart_toStartOf="@+id/EdiQtdLiq" />

    <EditText
        android:id="@+id/EdiQtdLiq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="67dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/EdiTickets"
        app:layout_constraintStart_toStartOf="@+id/EdiTickets"
        app:layout_constraintTop_toBottomOf="@+id/EdiValorLiquido" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="16dp"
        android:text="Insira o total de tickets"
        app:layout_constraintBottom_toTopOf="@+id/EdiTickets"
        app:layout_constraintStart_toStartOf="@+id/EdiTickets" />

    <EditText
        android:id="@+id/EdiValorLiquido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="134dp"
        android:layout_marginEnd="12dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/EdiTickets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="63dp"
        android:layout_marginTop="62dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EdiQtdLiq" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="27dp"
        android:text="Insira o total de clientes abordados"
        app:layout_constraintStart_toStartOf="@+id/EdiTickets"
        app:layout_constraintTop_toBottomOf="@+id/EdiTickets" />

    <EditText
        android:id="@+id/EdiAbordados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="Insira o tm Ideal"
        app:layout_constraintStart_toStartOf="@+id/EdiAbordados"
        app:layout_constraintTop_toBottomOf="@+id/EdiAbordados" />

    <EditText
        android:id="@+id/TMIDEAL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/btnsalvar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="39dp"
        android:backgroundTint="#6d120b"
        android:text="Salvar"
        app:layout_constraintEnd_toEndOf="@+id/PAIDEAL"
        app:layout_constraintTop_toBottomOf="@+id/PAIDEAL" />
</androidx.constraintlayout.widget.ConstraintLayout>